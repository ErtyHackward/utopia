<?xml version="1.0" encoding="utf-8" ?>
<GUILayout xmlns="http://ceguisharp.sourceforge.net/0.5/GUILayout"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://ceguisharp.sourceforge.net/0.5/GUILayout ..\..\..\CeGui\GUILayout.xsd">
	<Window Type="TaharezLook.TLFrameWindow" Name="MainMenu">
		<Property Name="Width" Value="0.4"/>
		<Property Name="Height" Value="0.8"/>
		<Property Name="TitlebarEnabled" Value="true"/>
		<Property Name="CloseButtonEnabled" Value="false"/>
		<Property Name="FrameEnabled" Value="true"/>
		<Property Name="RollUpState" Value="false"/>
		<Property Name="Text" Value="Main Menu" />
		<Window Type="TaharezLook.TLButton" Name="btnShowVideoModeSelection">
			<Property Name="XPosition" Value="0.17"/>
			<Property Name="YPosition" Value="0.15"/>
			<Property Name="Width" Value="0.65"/>
			<Property Name="Height" Value="0.07"/>
			<Property Name="Text" Value="Video Mode Selection" />
			<Event Name="Clicked" Function="OnShowVideoModeSelectionClicked"/>
		</Window>
		<Window Type="TaharezLook.TLButton" Name="btnExit">
			<Property Name="XPosition" Value="0.17"/>
			<Property Name="YPosition" Value="0.85"/>
			<Property Name="Width" Value="0.65"/>
			<Property Name="Height" Value="0.07"/>
			<Property Name="Text" Value="Exit" />
			<Event Name="Clicked" Function="OnExitClicked"/>
		</Window>
	</Window>
</GUILayout>